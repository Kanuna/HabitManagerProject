<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:bar="clr-namespace:HabitManager"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="800"
             x:Class="HabitManager.LandingView"
			 Background="Coral">



	<Grid ColumnDefinitions="*,2*,*" RowDefinitions="0.2*,*,*,AUTO">
		<Border Grid.Row="0" Grid.Column="0" BorderBrush="Black" BorderThickness="1" Padding="5"/>
		<Border Grid.Row="0" Grid.Column="1" BorderBrush="Black" BorderThickness="1" Padding="5"/>
		<Border Grid.Row="0" Grid.Column="2" BorderBrush="Black" BorderThickness="1" Padding="5"/>

		<!-- Second row -->
		<Border Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1" Padding="5"/>
		<Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="1" Padding="5"/>
		<Border Grid.Row="1" Grid.Column="2" BorderBrush="Black" BorderThickness="1" Padding="5"/>

		<Border Grid.Row="2" Grid.Column="0" BorderBrush="Black" BorderThickness="1" Padding="5"/>
		<Border Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="1" Padding="5"/>
		<Border Grid.Row="2" Grid.Column="2" BorderBrush="Black" BorderThickness="1" Padding="5"/>


		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<!-- Header for Days of the Week -->
			<RowDefinition Height="*"/>
			<!-- Dates -->
		</Grid.RowDefinitions>

		
		
		<!--<Calendar Grid.ColumnSpan="3" HorizontalAlignment="Stretch" Background="Black" Foreground="White" BorderThickness="0"/>-->
		<!--<bar:MenuBar Grid.ColumnSpan="3" Grid.Row="2"/>-->
	</Grid>

</UserControl>
